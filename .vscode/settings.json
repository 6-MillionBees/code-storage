    "editor.semanticTokenColorCustomizations": {
        "[the orange box]": {
            "enabled": true,
            "rules": {
                "method": "#f88b47",
                "method.builtin": "#f07c34",
                "method.declaration": "#ee8a4b",
                "module": {
                    "foreground": "#ff4848",
                    "fontStyle": "underline",
                },
                "variable": {
                    "foreground": "#63c4b9",
                    "fontStyle": ""
                },
                "builtinConstant": "#ce2dc0",

                "function": "#f89233",
                "function.builtin": "#ffac46",
                "function.declaration": {
                    "foreground": "#f19645",
                    "fontStyle": "italic bold"
                },

                "class": "#ffe14b",
                "parameter": "#04cc79",
                "property": "#e7e568"
            }
        },
        "[Dark Pink Pro]": {
            "enabled": true,
            "rules": {
                "method": "#d45c38",
                "module": {
                    "foreground": "#ff3679",
                    "fontStyle": "underline"
                },
                "variable": {
