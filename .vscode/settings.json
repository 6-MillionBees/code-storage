    "editor.tokenColorCustomizations": {
        "[the orange box]": {
            "comments": "#614f43",
            "numbers": "#bb4fa9",
            "strings": {
                "foreground": "#96bc70",
                "fontStyle": ""
            },
            "functions": {
                "foreground": "#f89f33",
                "fontStyle": ""
            },
            "keywords": {
                "foreground": "#e8571e",
                "fontStyle": "underline",
            },
            "types": {
                "foreground": "#f2601c",
                "fontStyle": ""
            },
            "textMateRules":[

                // Python Rules
                {
                    "scope": "",
                    "settings": {"foreground": "#FF0000", "fontStyle": "bold"}
                },


                // Markdown Rules
                {
                    "scope": "markup.heading.markdown entity.name.section.markdown",
                    "settings": { "foreground": "#43cab8",   "fontStyle": ""}
                },

                {
                    "scope": "",
                    "settings": {"foreground": "#FF0000", "fontStyle": "bold"}
                }
            ]
        },

        "[Dark Pink Pro]": {
            "comments": "#614f43",
            "numbers": "#e249c8",
            "strings": {
                "foreground": "#93e145",
                "fontStyle": ""
            },
            "functions": {
                "foreground": "#ff9361",
                "fontStyle": ""
            },
            "keywords": {
                "foreground": "#d45320",
                "fontStyle": "italic bold"
            },
            "types": {
                "foreground": "#ff891a",
                "fontStyle": ""
            }
        }
